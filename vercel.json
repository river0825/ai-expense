{
  "version": 2,
  "builds": [
    {
      "src": "dashboard/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://${{CLOUD_RUN_SERVICE_URL}}"
    },
    {
      "src": "/(.*)",
      "dest": "/dashboard/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "${CLOUD_RUN_SERVICE_URL}"
  }
}
